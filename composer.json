{
    "name": "defrostedtuna/viiidb-api",
    "type": "project",
    "description": "Final Fantasy VIII Informational Database.",
    "keywords": ["final", "fantasy", "final fantasy", "viii", "8", "api"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.4",
        "laravel/framework": "^9.3.1",
        "laravel/tinker": "^2.7",
        "sentry/sentry-laravel": "^2.11",
        "webpatser/laravel-uuid": "^4.0"
    },
    "require-dev": {
        "dms/phpunit-arraysubset-asserts": "^0.4.0",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": "vendor/bin/phpunit",
        "test:report": "vendor/bin/phpunit --coverage-html reports/",
        "phpstan": "vendor/bin/phpstan"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
